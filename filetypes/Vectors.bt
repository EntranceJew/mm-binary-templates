// -*- mode: c;-*-
typedef struct {
    short x;
    short y;
    short z;
} VEC3S[3] <read=Vec3SRead, write=Vec3SWrite>;

string Vec3SRead( VEC3S v ){
    string s;
    SPrintf( s, "(%f %f %f)", v.x, v.y, v.z );
    return s;
}

void Vec3SWrite( VEC3S &v, string s )
{
    SScanf( s, "(%f %f %f)", v.x, v[1], v[2] );
}
